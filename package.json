{
  "name": "vite-ionic-ts",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "build:sit": "vite build --mode sit && npx cap sync android && npx cap open android",
    "build:release": "vite build --mode production && npx cap sync android && npx cap open android",
    "android:release": "vite build && npx cap sync android && cd android && gradlew assembleRelease && npm run sign",
    "sign": "cd android/app/build/outputs/apk/release && jarsigner -keystore android/solamh.keystore -storepass 123456 app-release-unsigned.apk sola-h5 -signedjar android/app-release.apk"
  },
  "dependencies": {
    "@capacitor/android": "4.0.1",
    "@capacitor/app": "4.0.1",
    "@capacitor/core": "4.0.1",
    "@capacitor/splash-screen": "^4.0.1",
    "@capacitor/status-bar": "4.0.1",
    "@ionic/core": "^6.1.13",
    "@ionic/vue": "^6.1.13",
    "@ionic/vue-router": "^6.1.13",
    "axios": "^0.27.2",
    "moment": "^2.29.4",
    "pinia": "^2.0.20",
    "ts-md5": "^1.2.11",
    "vue": "^3.2.25",
    "vue-router": "^4.0.13"
  },
  "devDependencies": {
    "@capacitor/cli": "4.0.1",
    "@types/node": "^18.0.3",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "@vitejs/plugin-vue": "^2.3.3",
    "cordova-res": "0.15.4",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.2.0",
    "less": "^4.1.3",
    "less-loader": "^11.0.0",
    "prettier": "^2.7.1",
    "typescript": "^4.5.4",
    "vconsole": "^3.14.6",
    "vite": "^2.9.9",
    "vite-plugin-vconsole": "^1.2.2",
    "vue-tsc": "^0.34.7"
  }
}
